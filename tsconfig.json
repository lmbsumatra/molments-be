    "target": "ES2020",
    "module": "commonjs",
    "lib": ["es2020"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist"
    
  },
  "exclude": ["node_modules", "dist"]
}
