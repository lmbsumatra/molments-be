{
  "version": 2,
  "builds": [{ "src": "api/**/*.ts", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/api/users/([^/]+)",
      "dest": "/api/users/[id].ts?id=$1"
    },
    {
      "src": "/api/users",
      "dest": "/api/users/index.ts"
    },
    {
      "src": "/api/moments/([^/]+)",
      "dest": "/api/moments/[id].ts?id=$1"
    },
    {
      "src": "/api/moments",
      "dest": "/api/moments/index.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.ts"
    }
  ]
}
